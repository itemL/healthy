.container{
  height:1100rpx;
  left: 0rpx;
  top: 0rpx;
  position: relative;
  padding-left: 30rpx;
  padding-right: 30rpx;
  background-color: #f2f2f2;
}

.container .temp_container{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.temp_container .image{
  width: 200rpx;
  height: 200rpx;
}

.temp_container .title{
  font-size: 46rpx;
  line-height: 50rpx;
}

.none_container{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;

  height: 100%;
}

